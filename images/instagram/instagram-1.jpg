			/* ========================================
     KONTAKT PAGE STYLING
     ======================================== */

  .kontakt-wrapper {
    max-width: 1200px;
    margin: 0 auto;
    padding: 2rem 1rem;
  }

  .kontakt-subtitle {
    font-size: 1.875rem;
    font-weight: 600;
    color: #115e59;
    margin-bottom: 2rem;
    text-align: left;
  }

  /* Two-column grid */
  .kontakt-grid-2 {
    display: grid;
    grid-template-columns: 1fr;
    gap: 2rem;
    margin-bottom: 3rem;
  }

  @media (min-width: 768px) {
    .kontakt-grid-2 {
      grid-template-columns: 1fr 1fr;
    }
  }

  /* Salon cards */
  .kontakt-card {
    background: white;
    border-radius: 0.75rem;
    padding: 2rem;
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    transition: all 0.3s ease;
  }

  .kontakt-card:hover {
    box-shadow: 0 10px 15px rgba(0, 0, 0, 0.15);
    transform: translateY(-4px);
  }

  .kontakt-card-title {
    font-size: 1.5rem;
    font-weight: 600;
    color: #0f766e;
    margin-bottom: 1.5rem;
    padding-bottom: 0.75rem;
    border-bottom: 2px solid #14b8a6;
  }

  /* Info sections */
  .kontakt-info-section {
    margin-bottom: 1.5rem;
  }

  .kontakt-info-section:last-child {
    margin-bottom: 0;
  }

  .kontakt-info-section h4 {
    font-size: 0.875rem;
    font-weight: 700;
    color: #374151;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    margin-bottom: 0.5rem;
  }

  .kontakt-info-section p {
    font-size: 1rem;
    color: #6b7280;
    line-height: 1.6;
    margin: 0;
  }

  .kontakt-info-section a {
    color: #14b8a6;
    text-decoration: none;
    transition: color 0.2s ease;
  }

  .kontakt-info-section a:hover {
    color: #0d9488;
    text-decoration: underline;
  }

  .kontakt-note {
    font-style: italic;
    color: #9ca3af !important;
    margin-top: 0.5rem !important;
  }

  /* Contact Form Section */
  .kontakt-form-section {
    margin-bottom: 3rem;
  }

  .kontakt-form {
    background: white;
    border: 2px solid #e5e7eb;
    border-radius: 0.75rem;
    padding: 2rem;
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.05);
  }

  .kontakt-form-row {
    display: grid;
    grid-template-columns: 1fr;
    gap: 1.25rem;
    margin-bottom: 1.25rem;
  }

  @media (min-width: 768px) {
    .kontakt-form-row {
      grid-template-columns: 1fr 1fr;
    }
  }

  .kontakt-form-field label,
  .kontakt-form-field-full label {
    display: block;
    font-size: 0.875rem;
    font-weight: 600;
    color: #374151;
    margin-bottom: 0.5rem;
  }

  .kontakt-form-field input,
  .kontakt-form-field-full input,
  .kontakt-form-field-full textarea {
    width: 100%;
    padding: 0.75rem 1rem;
    font-size: 1rem;
    color: #111827;
    background: #ffffff;
    border: 2px solid #d1d5db;
    border-radius: 0.5rem;
    transition: all 0.2s ease;
  }

  .kontakt-form-field input:focus,
  .kontakt-form-field-full input:focus,
  .kontakt-form-field-full textarea:focus {
    outline: none;
    border-color: #14b8a6;
    box-shadow: 0 0 0 3px rgba(20, 184, 166, 0.1);
  }

  .kontakt-submit-btn {
    background: #2563eb;
    color: white;
    padding: 0.75rem 1.5rem;
    border: none;
    border-radius: 0.5rem;
    font-size: 1rem;
    font-weight: 600;
    cursor: pointer;
    transition: all 0.2s ease;
  }

  .kontakt-submit-btn:hover {
    background: #1d4ed8;
    box-shadow: 0 4px 6px rgba(37, 99, 235, 0.3);
    transform: translateY(-2px);
  }

  /* Maps Section */
  .kontakt-maps-section {
    margin-top: 3rem;
  }

  .kontakt-map-container {
    margin-bottom: 1.5rem;
  }

  .kontakt-map-title {
    font-size: 1.125rem;
    font-weight: 600;
    color: #0f766e;
    margin-bottom: 1rem;
  }

  .kontakt-map-wrapper {
    border-radius: 0.75rem;
    overflow: hidden;
    box-shadow: 0 10px 15px rgba(0, 0, 0, 0.1);
  }

  .kontakt-map-wrapper iframe {
    display: block;
    width: 100%;
  }

  /* Responsive */
  @media (max-width: 768px) {
    .kontakt-wrapper {
      padding: 1.5rem 1rem;
    }

    .kontakt-subtitle {
      font-size: 1.5rem;
    }

    .kontakt-card {
      padding: 1.5rem;
    }

    .kontakt-card-title {
      font-size: 1.25rem;
    }

    .kontakt-form {
      padding: 1.5rem;
    }
  }


